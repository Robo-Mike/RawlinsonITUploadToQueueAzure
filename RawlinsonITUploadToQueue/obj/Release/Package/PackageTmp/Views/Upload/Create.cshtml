
@{
    ViewBag.Title = "Create";
}

<h2>Upload a file</h2>
@using (Html.BeginForm("Create", "Upload", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <div class="progress-bar-success">@TempData["ResultMessage"] </div>

    <div class="form-horizontal">



        <div class="form-group">
            <label class="control-label col-md-2" for="file">Choose a file to upload</label>
            <div class="col-md-10">
                <input type="file" name="file" accept="text/*" class="form-control fileupload" />
            </div>
        </div>
        <div class="form-group">
            
            <div class="col-md-12">
                <input type="submit" name="submit"  />
            </div>
        </div>
    </div>

}


